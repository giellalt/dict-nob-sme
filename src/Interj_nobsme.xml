<?xml version="1.0" encoding="UTF-8"?>
<r id="nobsme" xml:lang="nob">
   <e>
      <lg>
         <l pos="Interj">se</l>
      </lg>
      <mg>
         <tg xml:lang="sme">
            <t pos="Interj" src="nj" usage="vd">gea</t>
            <xg>
               <x>Se, hunden halter!</x>
               <xt>Gea, beana skierbmu!</xt>
            </xg>
         </tg>
      </mg>
   </e>
   <e>
      <lg>
         <l pos="Interj">velkommen</l>
      </lg>
      <mg>
         <tg xml:lang="sme">
            <t pos="N" usage="vd">bures boahtin</t>
            <xg>
               <x>Velkommen til alle!</x>
               <xt>Bures boahtin buohkaide!</xt>
            </xg>
            <xg>
               <x>Jeg var velkommen hjem.</x>
               <xt>Mun ledjen bures boahtin ruoktot.</xt>
            </xg>
         </tg>
      </mg>
   </e>
   <e>
      <lg>
         <l pos="Interj">unnskyld</l>
      </lg>
      <mg>
         <tg xml:lang="sme">
            <t pos="Adv" src="nj" usage="vd">ándagassii</t>
            <xg>
               <x>Jeg tilgir.</x>
               <xt>Attán ándagassii.</xt>
            </xg>
         </tg>
      </mg>
   </e>
   <e>
      <lg>
         <l pos="Interj">gratulerer</l>
      </lg>
      <mg>
         <tg xml:lang="sme">
            <t pos="Interj" usage="vd">ollu lihkku</t>
            <xg>
               <x>Gratulerer med dagen!</x>
               <xt>Ollu lihkku beivviin!</xt>
            </xg>
         </tg>
      </mg>
   </e>
   <e>
      <lg>
         <l pos="Interj">pokker</l>
      </lg>
      <mg>
         <tg xml:lang="sme">
            <t pos="Adv" src="nj" usage="vd">doantá</t>
         </tg>
      </mg>
   </e>
   <e>
      <lg>
         <l pos="Interj">oi</l>
      </lg>
      <mg>
         <tg xml:lang="sme">
            <t pos="Interj" usage="vd">vuoi ihán</t>
            <xg>
               <x>Oi, så flink du er.</x>
               <xt>Vuoi ihán, don leat čeahppi.</xt>
            </xg>
            <xg>
               <x>Oi, det er rotete her.</x>
               <xt>Vuoi ihán, dáppe lea moivvas.</xt>
            </xg>
         </tg>
      </mg>
   </e>
   <e>
      <lg>
         <l pos="Interj">nei</l>
      </lg>
      <mg>
         <tg xml:lang="sme">
            <re>uttrykk for at en nekter, avviser eller protesterer mot noe</re>
            <t pos="Interj" usage="vd">aa</t>
            <xg>
               <x>Nei, det vil jeg ikke.</x>
               <xt>Aa, dan in háliit.</xt>
            </xg>
         </tg>
      </mg>
   </e>
   <e>
      <lg>
         <l pos="Interj">jasså</l>
      </lg>
      <mg>
         <tg xml:lang="sme">
            <t pos="Interj" usage="vd">já</t>
         </tg>
      </mg>
   </e>
   <e>
      <lg>
         <l pos="Interj">hei</l>
      </lg>
      <mg>
         <tg xml:lang="sme">
            <t pos="Interj" src="nj" usage="vd">dearvva</t>
         </tg>
      </mg>
   </e>
   <e>
      <lg>
         <l pos="Interj">akk</l>
      </lg>
      <mg>
         <tg xml:lang="sme">
            <t pos="Interj" src="nj" usage="vd">vuoi</t>
            <xg>
               <x>Akk, hvor trist det er.</x>
               <xt>Vuoi, go lea váivi.</xt>
            </xg>
         </tg>
      </mg>
   </e>
   <e>
      <lg>
         <l pos="Interj">å</l>
      </lg>
      <mg>
         <tg xml:lang="sme">
            <t pos="Interj" src="nj" usage="vd">vuoi</t>
            <xg>
               <x>Å, hvor trist det er.</x>
               <xt>Vuoi, go lea váivi.</xt>
            </xg>
            <xg>
               <x>Å, så artig at noen er redd for meg også.</x>
               <xt>Vuoi, man somá go soamis mus maid ballá.</xt>
            </xg>
         </tg>
      </mg>
   </e>
</r>
